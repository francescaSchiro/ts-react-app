{
  "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-react"],
  "rules": {
    // override tslint-react rules here
    "interface-name": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
